package main

import "fmt"

func main() {
	slice := []int{1, 3, 7, 8, 2, 0, -1, 9, 4, 100}
	max := -999999999999
	min := 10000000000000000
	for _, acessoslice := range slice {
		if acessoslice > max {
			max = acessoslice
		}
		if acessoslice < min {
			min = acessoslice
		}
	}
	fmt.Println(max, min)
}
